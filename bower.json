{
  "name": "template-dev-webapp",
  "version": "0.2.0",
  "main": "app/main.js",
  "moduleType": [
    "amd",
    "globals",
    "node"
  ],
  "authors": [
    "Jason Wohlgemuth"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "node_modules",
    "assets/library/bower_components",
    "test",
    "Gruntfile.js",
    "karma.conf.js",
    "package.json",
    ".gitignore",
    "*.md",
    "*.bat"
  ],
  "dependencies": {
    "marionette": "~2.4.1",
    "jquery": "~2.1.3",
    "handlebars": "~3.0.0",
    "backbone.radio": "~0.9.0"
  },
  "devDependencies": {
    "jasmine": "~2.2.1"
  },
  "preen": {
    "html5shiv": [
      "dist/*.js",
      "bower.json"
    ],
    "jquery": [
      "dist/*.js",
      "dist/*.map",
      "bower.json"
    ],
    "jasmine": [
      "lib/jasmine-core/*.*",
      ".bower.json"
    ],
    "backbone": [
      "backbone.js",
      "bower.json"
    ],
    "marionette": [
      "lib/*.js",
      "lib/*.map",
      "bower.json"
    ],
    "backbone.wreqr": [
      "lib/*.js",
      "bower.json"
    ],
    "backbone.babysitter": [
      "lib/*.js",
      "bower.json"
    ],
    "backbone.radio": [
      "build/*.js",
      "bower.json"
    ],
    "handlebars": [
      "handlebars.min.js",
      "bower.json"
    ]
  },
  "overrides": {
    "jquery": {
      "main": "dist/jquery.min.js"
    },
    "underscore": {
      "main": "underscore-min.js"
    },
    "marionette": {
      "main": "lib/backbone.marionette.min.js"
    },
    "backbone.wreqr": {
      "main": "lib/backbone.wreqr.min.js"
    },
    "backbone.babysitter": {
      "main": "lib/backbone.babysitter.min.js"
    },
    "backbone.radio": {
      "main": "build/backbone.radio.min.js"
    },
    "handlebars": {
      "main": "handlebars.min.js"
    }
  }
}
